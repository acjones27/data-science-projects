tryCatch({
    Sys.setenv(RSTUDIO_PANDOC = "/usr/lib/rstudio-server/bin/pandoc")
    rmarkdown::render(paste0(filepath,"src/report_template.Rmd"), output_format = "html_document", runtime = "shiny_prerendered")
  },error = function(e){
    tryCatch({
      Sys.setenv(RSTUDIO_PANDOC = "/usr/lib/rstudio-server/bin/pandoc/pandoc")
      rmarkdown::render(paste0(filepath,"src/report_template.Rmd"), output_format = "html_document", runtime = "shiny_prerendered")
    }, error = function(e){
      Sys.setenv(RSTUDIO_PANDOC = "/usr/lib/rstudio/bin/pandoc")
      rmarkdown::render(paste0(filepath,"src/report_template.Rmd"), output_format = "html_document", runtime = "shiny_prerendered")
    })
  })